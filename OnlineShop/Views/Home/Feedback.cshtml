@model List<Model.EF.Feedback>
<div class="block block__feedback">
    <div class="block__header">
        <span class="block__header__title">ĐÁNH GIÁ TỪ KHÁCH HÀNG</span>
    </div>

    <div class="block__content desktop">
        <div class="swiper-container feedback">
            <div class="swiper-wrapper">
                @foreach (var item in Model)
                {
                    <div class="swiper-slide">
                        <div class="swiper-slide-img-wrapper">
                            <img src="@item.Image">
                        </div>
                    </div>
                }

            </div>
            <!-- Add Pagination -->
            <div class="swiper-pagination swiper-pagination-white"></div>
            <!-- Add Arrows -->
            <div class="swiper-button-next swiper-button-white"></div>
            <div class="swiper-button-prev swiper-button-white"></div>
        </div>
    </div>
</div>
